      <!-- Languages -->
      <div class="module">
	<h3>Language</h3>      
	<p>
	  
	  <a href="/ga/education/third_level_education/colleges_and_qualifications/third_level_education_in_ireland.html" title="As Gaeilge" hreflang="ga" lang="ga">Gaeilge</a>
	  
	  
	</p>		
      </div>
      
      

      
      


           
